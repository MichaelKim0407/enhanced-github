{
    "manifest_version": 2,
    "name": "Enhanced GitHub",
    "version": "0.1",
    "permissions": [
        "storage"
    ],
    "options_page": "options/options.html",
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*.ipynb"
            ],
            "js": [
                "lib/jquery-3.3.1/jquery.min.js",
                "util.js",
                "jupyter/btn.js"
            ]
        },
        {
            "matches": [
                "https://render.githubusercontent.com/view/ipynb",
                "https://render.githubusercontent.com/view/ipynb?*"
            ],
            "all_frames": true,
            "css": [
                "jupyter/scroll.css"
            ],
            "js": [
                "lib/jquery-3.3.1/jquery.min.js",
                "util.js",
                "jupyter/table.js",
                "jupyter/scroll.js"
            ]
        }
    ]
}
