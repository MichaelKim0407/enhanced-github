{
    "manifest_version": 2,
    "name": "Enhanced GitHub",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "lib/jquery-3.3.1/jquery.min.js"
            ]
        },
        {
            "matches": [
                "https://render.githubusercontent.com/view/ipynb",
                "https://render.githubusercontent.com/view/ipynb?*"
            ],
            "all_frames": true,
            "js": [
                "lib/jquery-3.3.1/jquery.min.js",
                "util.js",
                "jupyter/table.js"
            ]
        }
    ]
}
